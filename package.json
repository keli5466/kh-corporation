{
	"name": "kittyhawk",
	"version": "0.0.1",
	"author": "Bukwild",
	"repository": "git@gitlab.com:bukwild/kitty-hawk/corporate.git",
	"license": "UNLICENSED",
	"devDependencies": {
		"benchpress": "bkwld/benchpress",
		"coffee-script": "^1.10.0",
		"dotenv-webpack": "^1.5.7",
		"request-promise": "^4.2.1",
		"shipit-cli": "^3.0.0",
		"shipit-deploy": "~2.4.0",
		"webpack": "2 - 3",
		"webpack-dashboard": "^0.4.0",
		"webpack-dev-server": "~2.6.1"
	},
	"scripts": {
		"setup": "coffee node_modules/benchpress/tools/setup.coffee",
		"dev": "printf \"\\e]2;$(cd ../ && basename $(pwd))\\a\" && printf \"\\e]1;Webpack - Dev\\a\" && webpack-dashboard -- webpack-dev-server",
		"watch": "printf \"\\e]2;$(cd ../ && basename $(pwd))\\a\" && printf \"\\e]1;Webpack - Watch\\a\" && webpack --watch",
		"minify": "NODE_ENV=production webpack --progress",
		"deploy:staging": "shipit staging deploy",
		"deploy:production": "shipit production deploy",
		"rollback:staging": "shipit staging rollback",
		"rollback:production": "shipit production rollback",
		"export": "coffee export.coffee"
	},
	"dependencies": {
		"@sentry/browser": "^4.2.3",
		"@vimeo/player": "^2.2.1",
		"balance-text": "^3.1.1",
		"bkwld-js-library": "bkwld/js-library",
		"body-scroll-toggle": "^0.1.2",
		"bukwild-stylus-library": "^0.4.3",
		"fastclick": "^1.0.6",
		"fittextjs": "^1.2.0",
		"jquery-match-height": "^0.7.2",
		"lodash": "^4.17.4",
		"normalize.css": "^7.0.0",
		"scrollmonitor-parallax": "^1.1.1",
		"slick-carousel": "^1.6.0",
		"velocity-animate": "^1.5.2",
		"vue": "^2.4.2",
		"vue-in-viewport-directive": "^1.1.4",
		"vue-in-viewport-mixin": "^1.0.3",
		"vue-visual": "^0.2.0",
		"window-event-mediator": "^0.3.1"
	}
}
